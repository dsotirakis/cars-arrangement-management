doctype html
html
  head
    title Add / Delete Cars
  body
    h1 Add / Delete Car
      br

      if categories.length > 0
        h2 Delete Car
        form(method = 'post', action = '/groups')
          select(id = "category", name = "categoryName", size = categories.length)
            each category in categories
              option(value=category.categoryName) #{category.categoryName}
          br
          input(type= "submit", class = "category", value = "Choose Group to view")

        br
      br
